	"version": "0.2.0",
	"configurations": [

		{
			"name": "QEMU launch qflex",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/qemu/aarch64-softmmu/qemu-system-aarch64",
			"args": [
				"-machine", "virt,gic-version=3",
				"-cpu", "cortex-a57",
				"-smp", "1", "-m", "8G",
				"-global", "virtio-blk-device.scsi=off",
				"-device", "virtio-scsi-device,id=scsi",
				"-rtc", "clock=vm", "-nographic",
				"-drive", "if=none,file=/home/ulises/Projects/QFlex/qflex-base/images/ubuntu16/ubuntu.qcow2,id=hd0",
				"-pflash", "/home/ulises/Projects/QFlex/qflex-base/images/ubuntu16/flash0.img", 
				"-pflash", "/home/ulises/Projects/QFlex/qflex-base/images/ubuntu16/flash1.img",
				"-device", "scsi-hd,drive=hd0",
				"-device", "virtio-scsi-device",
				"-netdev", "user,id=net1,hostfwd=tcp::2239-:22",
				"-device", "virtio-net-device,mac=52:54:00:00:00:00,netdev=net1",
				"-icount", "shift=0,sleep=off,align=off",
				"-singlestep", "-d", "nochain,int",
				"-flexus", "mode=trace,length=1000000,simulator=/home/ulises/Projects/QFlex/qflex-base/flexus/libKeenKraken.so,config=/home/ulises/Projects/QFlex/qflex-base/flexus/user_postload,debug=dev",
				"-loadext", "logged",
				"-qflex_d", "loop=20,gen", "-ckpt", "every=500000,end=1000000",
				"-exton",
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/qemu/aarch64-softmmu",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
			    {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			    },
			    {
				"description":  "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			    }
			],
		},

